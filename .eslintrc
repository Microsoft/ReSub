{
  "parserOptions": {
    "project": [
        "./tsconfig.json",
        "./tsconfig/test.json"
    ]
  },
  "extends": [
    "skype", "skype/react"
  ],
  "env": {
    "jasmine": true
  },
  "rules": {},
  "overrides": [
    {
      "files": [
        "src/tslint/*.ts"
      ],
      "rules": {
        "no-console": "off"
      }
    },
    {
      "files": [
        "test/*"
      ],
      "rules": {
        "@typescript-eslint/camelcase": ["error", { "properties": "never", "allow": ["^test_[A-Za-z]+$"] }]
      }
    }
  ]
}
